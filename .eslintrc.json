{
  "extends": [
    "react-app",
    "prettier",
    "prettier/flowtype",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", {
      "useTabs": false,
      "printWidth": 120,
      "tabWidth": 4,
      "singleQuote": true,
      "trailingComma": "es5",
      "jsxBracketSameLine": true,
      "parser": "flow"}],
    "no-unused-vars": 2,
    "no-console": 2
  }
}
